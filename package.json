{
    "name": "markdown-parser",
    "release": "Markdown Parser library",
    "version": "3.3.7",
    "description": "Markdown Parser library, with Slack support",
    "main": "src/index.ts",
    "keywords": [
        "markdown",
        "slack",
        "parser"
    ],
    "author": "Yuriy Gorvitovskiy",
    "license": "SEE LICENSE IN LICENSE file",
    "repository": {
        "type": "git",
        "url": "https://github.com/YuriyGorvitovskiy/markdown-parser.git"
    },
    "bugs": {
        "url": "https://github.com/YuriyGorvitovskiy/markdown-parser/issues"
    },
    "homepage": "https://github.com/YuriyGorvitovskiy/markdown-parser",
    "scripts": {
        "upgrade-dep": "ncu -u && npm install --shrinkwrap",
        "compile": "tsc",
        "format": "prettier --write \"src/**/*.ts\" .prettierrc jest.config.js package.json tsconfig.json eslint.json",
        "lint": "tslint -p tsconfig.json",
        "test": "jest --coverage",
        "test-watch": "jest --coverage --watchAll",
        "check": "npm run preprocess && npm run format && npm run compile && npm run lint && npm run test",
        "package": "npm run preprocess && npm run format && npm run compile && npm run lint && npm run test && npm run docs && npm run webpack"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/jest": "26.0.18",
        "@types/node": "14.14.11",
        "eslint": "7.15.0",
        "jest": "26.6.3",
        "npm-check-updates": "10.2.3",
        "prettier": "2.2.1",
        "ts-jest": "26.4.4",
        "ts-loader": "8.0.11",
        "ts-node": "9.1.1",
        "typescript": "4.1.2"
    },
    "files": [
        "src",
        "*.js",
        "*.json",
        "*.ts"
    ]
}
