{
    "name": "markdown-parser",
    "release": "Markdown Parser library",
    "version": "3.3.12",
    "description": "Markdown Parser library, with Slack support",
    "main": "src/index.ts",
    "keywords": [
        "markdown",
        "slack",
        "parser"
    ],
    "author": "Yuriy Gorvitovskiy",
    "license": "SEE LICENSE IN LICENSE file",
    "repository": {
        "type": "git",
        "url": "https://github.com/YuriyGorvitovskiy/markdown-parser.git"
    },
    "bugs": {
        "url": "https://github.com/YuriyGorvitovskiy/markdown-parser/issues"
    },
    "homepage": "https://github.com/YuriyGorvitovskiy/markdown-parser",
    "scripts": {
        "upgrade-dep": "ncu -u && npm install --shrinkwrap",
        "compile": "tsc",
        "format": "prettier --write \"src/**/*.ts\" .prettierrc jest.config.json package.json tsconfig.json .eslintrc.json",
        "lint": "eslint src/**/*.ts",
        "test": "jest --coverage",
        "test-watch": "jest --coverage --watch",
        "check": "npm run format && npm run compile && npm run lint && npm run test"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/jest": "26.0.19",
        "@types/node": "14.14.13",
        "@typescript-eslint/eslint-plugin": "4.9.1",
        "@typescript-eslint/parser": "4.9.1",
        "eslint": "7.15.0",
        "eslint-config-prettier": "7.0.0",
        "eslint-plugin-prettier": "3.2.0",
        "jest": "26.6.3",
        "npm-check-updates": "10.2.5",
        "prettier": "2.2.1",
        "ts-jest": "26.4.4",
        "ts-loader": "8.0.12",
        "ts-node": "9.1.1",
        "typescript": "4.1.3"
    }
}
